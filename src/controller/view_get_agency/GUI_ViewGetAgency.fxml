<?import javafx.scene.layout.*?>

<BorderPane fx:id="root" prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.view_get_agency.ViewGetAgencyController">
         <bottom>
      <ToolBar rotate="180.0">
         <items>
                  <HBox alignment="CENTER_RIGHT" rotate="180.0">
                     <children>
                        
                              <Button alignment="CENTER" defaultButton="true" mnemonicParsing="false" onAction="#finishEventHandler" text="Add">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin></Button>
                  <Button defaultButton="true" mnemonicParsing="false" onAction="#cancelEventHandler" text="Exit">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
                     </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
                  </HBox>
         </items>
      </ToolBar>
         </bottom>
   <top>
      <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <items>
            <Button mnemonicParsing="false" onAction="#createAgencyEventHandler" text="Register a new agency" />
         </items>
      </ToolBar>
   </top>
   <center>
      <Pagination fx:id="pgnt_agency" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
